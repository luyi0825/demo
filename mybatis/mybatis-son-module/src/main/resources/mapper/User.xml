<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mybatis.son.mapper.UserMapper">

        <select id="selectById" parameterType="integer" resultType="com.demo.mybatis.son.entity.User">
            select id,name from user where id=#{id}
        </select>
</mapper>
